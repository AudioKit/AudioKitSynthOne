platform :ios, '9.0'
use_frameworks!

def available_pods
    pod 'Disk', '~> 0.3.2'
    pod 'Audiobus'
    pod 'ChimpKit'
    pod 'OneSignal', '>= 2.6.2', '< 3.0'
end

target 'AudioKitSynthOne' do
    available_pods 
end

target 'OneSignalNotificationServiceExtension' do
  pod 'OneSignal', '>= 2.6.2', '< 3.0'
end